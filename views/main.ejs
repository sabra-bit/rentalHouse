        <button class="btn btn-outline-secondary" type="submit" id="button-addon2">بحث</button>
      
      </div>
    </form>
      <div id="autocomplete1" name="autocomplete1" ></div>
    </div>
      <nav class="d-flex align-items-center">

        <% if (roll) { %>
          <% if (roll== 'admin') { %>

            <a href="/admin" class="text-light text-decoration-none px-3">مديرالنظام</a>
            <% } %>
          <% } %>
        <a href="/" class="text-light text-decoration-none px-3">الصفحة الرئيسية</a>
