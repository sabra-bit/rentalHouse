        <a href="#div_id" class="text-light text-decoration-none px-3">بيانات الاتصال</a>
        <% if (name) { %>
        <a href="/user" class="btn btn-light btn-sm ms-3"><%= name %></a>  
        <a href="/logout" class="btn btn-light btn-sm ms-3">تسجيل الخروج</a>
        <% } else{ %>  
